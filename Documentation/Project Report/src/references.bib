    pages = {1--4},
    file = {IEEE Xplore Abstract Record:/Users/noble/Zotero/storage/ZR72QZ26/8117934.html:text/html;IEEE Xplore Full Text PDF:/Users/noble/Zotero/storage/MCK82EVI/Chattoraj et al. - 2017 - Design and implementation of low cost electronic t.pdf:application/pdf},
}

