    author = {Rumbaugh, James and Rumbaugh, Jim and Blaha, Stephen and Blaha, Michael and Premerlani, William and Blaha, Michael R. and Eddy, Frederick and Lorensen, Bill and Rumbaugh, James R. and Lorensen, William},
